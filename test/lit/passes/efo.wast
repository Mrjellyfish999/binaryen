;; Four fields.
(module
  ;; CHECK:      (type $A (struct (field i32) (field i32) (field i32) (field i32)))
  (type $A (struct (field i32) (field i32) (field i32) (field i32)))

  ;; CHECK:      (func $A (type $i32_=>_none) (param $x i32)
